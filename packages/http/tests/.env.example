# Turnkey API credentials for testing
# Copy this file to .env and fill in your actual values

# Your Turnkey API public key
TURNKEY_API_PUBLIC_KEY=

# Your Turnkey API private key
TURNKEY_API_PRIVATE_KEY=

# Your Turnkey organization ID
TURNKEY_ORGANIZATION_ID=

# A valid user ID in your organization (for command tests)
TURNKEY_USER_ID=

# Turnkey API base URL (optional, defaults to https://api.turnkey.com)
TURNKEY_BASE_URL=https://api.turnkey.com
